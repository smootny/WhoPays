{
  "expo": {
    "name": "WhoPays",
    "slug": "WhoPays",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios"
    ],
    "scheme": "whopays",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/login-image.png",
      "resizeMode": "contain",
      "backgroundColor": "#DB4457"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sebastianblazejewski.whopays",
      "buildNumber": "1",
      "infoPlist": { "ITSAppUsesNonExemptEncryption": false }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "enabled": false
    }
  }
}
